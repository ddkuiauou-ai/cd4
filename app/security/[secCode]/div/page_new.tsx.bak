            { label: '5ÎÖÑ ÌèâÍ∑†', months: 60, desc: 'ÏµúÍ∑º 5ÎÖÑ' },
            { label: '10ÎÖÑ ÌèâÍ∑†', months: 120, desc: 'ÏµúÍ∑º 10ÎÖÑ' },
            { label: '20ÎÖÑ ÌèâÍ∑†', months: 240, desc: 'ÏµúÍ∑º 20ÎÖÑ' }
